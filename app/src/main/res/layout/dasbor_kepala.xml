<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.abdulwahab.mmt.com.example.abdulwahab.SIBAYU.Activity.DasborKepala">

    <android.support.v7.widget.Toolbar
        android:id="@+id/menu_toolbar_kepala"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@color/bgKedua"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        app:theme="@style/AppTheme">
    </android.support.v7.widget.Toolbar>

    <com.example.abdulwahab.mmt.Tab.SlidingTabLayout
        android:id="@+id/menu_stLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </com.example.abdulwahab.mmt.Tab.SlidingTabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/menu_vpTab"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
    </android.support.v4.view.ViewPager>
</LinearLayout>
